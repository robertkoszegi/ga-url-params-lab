<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Albums</title>
</head>
<body>
    <h1>Albums</h1>
    <ul>
      <% albums.forEach(function(a) { %>
      <li>
        <a href="/albums/<%= a.id %>"><%= a.name %></a> - <%= a.artist %>
      </li>
      <% }); %>
    </ul>
</body>
</html>